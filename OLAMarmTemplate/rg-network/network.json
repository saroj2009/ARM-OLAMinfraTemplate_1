      "dependsOn": [ ],
      "tags": {
        "displayName": "olamvn01"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('olamvn01Prefix')]"
          ]
        },
        "subnets": [
